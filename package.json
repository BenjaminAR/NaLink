{
  "name": "nalink",
  "displayName": "NaLink",
  "publisher": "BenjaminAvilaRivera",
  "description": "Shorten your URLs quickly and easily with NaLink!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "vscode",
    "extension",
    "url",
    "shorten",
    "tinyurl"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:extension.shortenUrl"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.shortenUrl",
        "title": "NaLink: Shorten URL"
      }
    ],
    "keybindings": [
      {
        "command": "extension.shortenUrl",
        "key": "ctrl+shift+alt+s",
        "mac": "cmd+shift+alt+s",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "NaLink",
      "properties": {
        "nalink.accessToken": {
          "type": "string",
          "default": "",
          "description": "Access token for TinyURL API"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "vscode:prepublish": "npm run lint"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.34",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "@vscode/test-electron": "^2.3.2"
  },
  "dependencies": {
    "axios": "^1.7.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BenjaminAR/NaLink.git"
  },
  "bugs": {
    "url": "https://github.com/BenjaminAR/NaLink/issues"
  },
  "homepage": "https://github.com/BenjaminAR/NaLink#readme",
  "license": "MIT"
}